{
  "name": "vue-grabbing-box",
  "version": "0.0.3",
  "description": "vue-grabbing-box is a container component that allows you to grab to change view-box and also with scaling",
  "files": [
    "dist",
    "./packages/utils",
    "./packages/vue2/src",
    "README**.md"
  ],
  "main": "./dist/vue2/index.js",
  "exports": {
    ".": {
      "import": "./dist/vue3/index.js",
      "require": "./dist/vue3/index.umd.cjs"
    },
    "./vue2": {
      "import": "./dist/vue2/index.js",
      "require": "./dist/vue2/index.umd.cjs"
    },
    "./vue3": {
      "import": "./dist/vue3/index.js",
      "require": "./dist/vue3/index.umd.cjs"
    },
    "./css": "./dist/**/**.css"
  },
  "scripts": {
    "dev": "echo TODO",
    "build": "pnpm -F @vue-grabbing-box/vue* build",
    "build:publish": "pnpm build && pnpm publish"
  },
  "keywords": [
    "canvas",
    "svg",
    "drag",
    "scale",
    "zoom",
    "grab",
    "touch",
    "container",
    "vue",
    "画布",
    "画布拖拽",
    "画布缩放",
    "多指缩放"
  ],
  "author": "maoniaiyou",
  "repository": {
    "type": "git",
    "url": "https://github.com/CoderMonkie/vue-grabbing-box.git"
  },
  "homepage": "https://github.com/CoderMonkie/vue-grabbing-box.git",
  "license": "ISC",
  "devDependencies": {
    "sass": "^1.62.1",
    "vite": "^4.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}